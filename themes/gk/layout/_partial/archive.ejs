<% switch (type) {
    case 'archive':
    var last;
    page.posts.each(function(post, i){
    var year = post.date.year();
    if (last != year){
      if (last != null){ %>
        </div></section>
      <% }
      last = year; %>

      <section class="archives-wrap">
        <div class="archive-year-wrap">
          <h2 class="page-title">
          <a href="<%- url_for('archives/' + year) %>" class="archive-year"><%= year %></a>
        </h2>
        </div>
        <div class="archives">
    <% } %>
    <%- partial('archive-post', {post: post, archive: true}) %>
  <% });
    if (page.posts.length){ %>
    </div></section>
  <% }
    if (page.total > 1){ %>
      <nav class="page-nav">
        <%- paginator({
          prev_text: '&laquo; ' + __('nav.prev'),
          next_text: __('nav.next') + ' &raquo;'
        }) %>
      </nav>
    <% }
    break;
  case 'category': %>
    <section class="archives-wrap">
    <h2 class="page-title">
      <span class="archive-category"><%= page.category %></span>
      
    </h2>
    <div class="archives">
    <% page.posts.each(function (post, i) { %>
      <%- partial('archive-post', {post: post, archive: false}) %>
    <% }); %>
    </div></section>
    <% if (page.total > 1){ %>
      <nav class="page-nav">
        <%- paginator({
          prev_text: '&laquo; ' + __('nav.prev'),
          next_text: __('nav.next') + ' &raquo;'
        }) %>
      </nav>
    <% }
    break;
  case 'tag': %>
    <section class="archives-wrap">
    <h2 class="page-title">
      <span class="archive-tag">#<%= page.tag %></span>
    </h2>
    <div class="archives">
    <% page.posts.each(function (post, i) { %>
      <%- partial('archive-post', {post: post, archive: false}) %>
    <% }); %>
    </div></section>
    <% if (page.total > 1){ %>
      <nav class="page-nav">
        <%- paginator({
          prev_text: '&laquo; ' + __('nav.prev'),
          next_text: __('nav.next') + ' &raquo;'
        }) %>
      </nav>
    <% }
    break;
  default:
    page.posts.each(function(post){ %>
      <%- partial('article', {post: post, index: true}) %>
    <% })
    if (page.total > 1){ %>
      <nav class="page-nav">
        <%- paginator({
          prev_text: '&laquo; ' + __('nav.prev'),
          next_text: __('nav.next') + ' &raquo;'
        }) %>
      </nav>
    <% }
    break;
} %>