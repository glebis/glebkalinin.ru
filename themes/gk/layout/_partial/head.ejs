<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <%
  var title = page.title;

  if (is_archive()){
    title = 'Архив';

    if (is_month()){
      title += ': ' + page.year + '/' + page.month;
    } else if (is_year()){
      title += ': ' + page.year;
    }
  } else if (is_category()){
    title = 'Категория: ' + page.category;
  } else if (is_tag()){
    title = 'Метка: ' + page.tag;
  }
  %>
  <title><% if (title){ %><%= title %> - <% } %><%= config.title %></title>
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

  <%- open_graph({twitter_id: theme.twitter, google_plus: theme.google_plus, fb_admins: theme.fb_admins, fb_app_id: theme.fb_app_id}) %>
  <% if (theme.rss){ %>
    <link rel="alternative" href="<%- theme.rss %>" title="<%= config.title %>" type="application/atom+xml">
  <% } %>
  <% if (theme.favicon){ %>
    <link rel="icon" href="<%- theme.favicon %>">
  <% } %>
  
  <link rel="stylesheet" href="https://fonts.typotheque.com/WF-010357-009483.css" type="text/css" />
  <%- css('css/style') %>
  <%- partial('google-analytics') %>
  <link rel="canonical" href="<%= url.replace('/index.html','/') %>">
  <% if (is_post() && config.generator_amp && site.data ){ %>
    <!-- GOOGLE AMP -->
    <link rel="amphtml" href="<%= config.url %>/<%= page.path %>amp/index.html">
  <% } %>
</head>

